<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="MyDMG_app.AppShell"
    xmlns:views="clr-namespace:MyDMG_app.Views">

    <ShellContent Route="LoginPage"
                  ContentTemplate="{DataTemplate views:LoginPage}" />
    <ShellContent Route="HomePage"
                  ContentTemplate="{DataTemplate views:HomePage}" />
    <ShellContent Route="RegisterPage"
                  ContentTemplate="{DataTemplate views:RegisterPage}" />
    <ShellContent Route="CrearCortejoPage"
                  ContentTemplate="{DataTemplate views:CrearCortejoPage}" />



</Shell>
 
    